generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products {
  id            Int           @id @default(autoincrement())
  name          String        @db.Text
  description   String        @db.Text
  price         Int
  quantity      Int
  category_id   Int
  created_at    DateTime      @default(now()) @db.Timestamp
  updated_at    DateTime?     @default(now()) @db.Timestamp
}
